def sorting(nums):
  for i in range(0, len(nums)):
    cur_min_idx = i
    
    for j in range(i , len(nums)):
      if nums[cur_min_idx] > nums[j]:
        nums[cur_min_idx], nums[j] = nums[j], nums[cur_min_idx]
        
  return nums

# Corrected usage
nums = [3,8,1,9,2,10,0]


# Call the twoSum function and print the result
print(sorting(nums))
