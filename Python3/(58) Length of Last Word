 def lengthOfLastWord(self, s: str) -> int:
        # count = 0

        # my_list = s.split()
        # print(my_list)

        # last_word = my_list[-1]

        # for char in last_word:
        #     count += 1

        # return count

# TIme: O(n) n is the length of string because we use split()
# Space: O(k) k is the number of words in string cause we need k space to store for split()

        words = s.split()

        if not words:
            return 0
        
        return len(words[-1])
